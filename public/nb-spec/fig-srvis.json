{
  "data": [
    {
      "name": "houses",
      "path": "./data/houses.json"
    }
  ],
  "layout": {
    "width": "1550px",
    "height": "1250px",
    "rows": ["100px", "150px", "800px", "150px"],
    "columns": ["100px", "150px", "800px", "150px", "1fr"]
  },
  "visualizations": [
    {
      "id": "histo-top",
      "container": "1 1 3 3",
      "visualization": "barchart",
      "props": {
        "data": "houses",
        "encoding": {
          "x": "lng1",
          "aggregate": "count",
          "count": 20,
          "bottomEdge": "bottom",
          "isDisplay": true
        }
      }
    },
    {
      "id": "histo-left",
      "container": "3 3 1 1",
      "visualization": "barchart",
      "props": {
        "data": "houses",
        "encoding": {
          "x": "lat1",
          "aggregate": "count",
          "count": 20,
          "bottomEdge": "right",
          "isDisplay": false
        }
      }
    },
    {
      "id": "stackedbar-right",
      "container": "3 3 4 4",
      "visualization": "barchart",
      "props": {
        "data": "houses",
        "encoding": {
          "x": "lat1",
          "y": ["Living Rooms", "Bedrooms"],
          "aggregate": ["count", "count"],
          "count": 20,
          "bottomEdge": "left",
          "stacked": true,
          "isDisplay": false
        }
      }
    },
    {
      "id": "stackedbar-bottom",
      "container": "4 4 3 3",
      "visualization": "barchart",
      "props": {
        "data": "houses",
        "encoding": {
          "x": "lng1",
          "y": ["Living Rooms", "Bedrooms", "Floor Size"],
          "aggregate": ["average", "average", "average"],
          "count": 20,
          "color": "#104BA9",
          "selectionColor": "#FFA200",
          "bottomEdge": "top",
          "stacked": true,
          "isDisplay": false
        }
      }
    },
    {
      "id": "map",
      "container": "3 3 3 3",
      "visualization": "map",
      "props": {
        "data": "houses",
        "x": "lng1",
        "y": "lat1", 
        "brushType": "xy",
        "bottomEdge": "bottom",
        "mapStyle": {
          "minZoom": 3,
          "maxZoom": 20,
          "center": [30.2708376789, 120.130177269],
          "zoom": 10,
          "zoomControl": false,
          "attributionControl": false
        }
      }
    },
    {
      "id": "lineup",
      "container": "1 4 5 5",
      "visualization": "lineup",
      "props": {
        "data": "houses",
        "order": ["Living Rooms", "Bedrooms", "Floor Size"]
      }
    }
  ],
  "coordinations": [
    {
      "how": "select items in any, then select items in others",
      "visualizations": ["histo-top", "histo-left", "stackedbar-right", "stackedbar-bottom", "map"]
    },
    {
      "how": "select ranges in any, then select ranges in others",
      "visualizations": ["histo-top", "histo-left", "stackedbar-right", "stackedbar-bottom", "map"]
    },
    "navigate ranges in map, then navigate ranges in histo-top, histo-left, stackedbar-right, stackedbar-bottom",
    "reconfigure order in lineup, then encode y in stackedbar-right, stackedbar-bottom"
  ]
}
