{
  "data": [
    {
      "name": "covid",
      "path": "./data/daily-cases-covid-19-by-continent.json"
    }
  ],
  "layout": {
    "width": "300px",
    "height": "500px",
    "rows": ["1fr", "1fr"],
    "columns": ["1fr"]
  },
  "visualizations": [
    {
      "id": "chart1",
      "container": "1 1 1 1",
      "visualization": "linechart",
      "props": {
        "data": "covid",
        "x": "Date",
        "y": "Daily new confirmed cases (cases)",
        "detail": "Entity",
        "brushType": "x",
        "colors": [
          "#4e79a7",
          "#f28e2c",
          "#e15759",
          "#76b7b2",
          "#59a14f",
          "#edc949"
        ]
      }
    },
    {
      "id": "chart2",
      "container": "2 2 1 1",
      "visualization": "piechart",
      "props": {
        "data": "covid",
        "range": "Entity",
        "value": "Daily new confirmed cases (cases)",
        "aggregate": "sum",
        "colors": [
          "#4e79a7",
          "#f28e2c",
          "#e15759",
          "#76b7b2",
          "#59a14f",
          "#edc949"
        ]
      }
    }
  ],
  "coordinations": ["select in chart1, then set data in chart2"]
}
