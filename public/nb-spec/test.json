{
  "data": [
    {
      "name": "points",
      "values": [
        { "A": 3.2, "B": 3.7, "C": 4.2 },
        { "A": 3.5, "B": 3.7, "C": 4.9 },
        { "A": 3.9, "B": 3.8, "C": 6.2 },
        { "A": 3.2, "B": 3.0, "C": 5.3 },
        { "A": 3.6, "B": 3.0, "C": 4.4 },
        { "A": 3.7, "B": 3.6, "C": 4.7 },
        { "A": 3.2, "B": 4.4, "C": 5.2 },
        { "A": 3.5, "B": 3.9, "C": 6.0 },
        { "A": 3.1, "B": 3.2, "C": 4.0 },
        { "A": 3.0, "B": 3.3, "C": 4.9 }
      ]
    },
    {
      "name": "miserables",
      "path": "./data/miserables.json"
    },
    {
      "name": "iris",
      "path": "./data/iris.csv",
      "format": "csv"
    }
  ],
  "layout": {
    "width": "1600px",
    "height": "1000px",
    "rows": ["500px", "1fr"],
    "columns": ["500px", "1fr", "500px"]
  },
  "visualizations": [
    {
      "id": "chart1",
      "container": "1 1 1 1",
      "visualization": "scatterplot",
      "props": {
        "data": "points",
        "x": "A",
        "y": "C"
      }
    },
    {
      "id": "chart2",
      "container": "2 2 1 1",
      "visualization": "lineup",
      "props": {
        "data": "points"
      }
    },
    {
      "id": "chart3",
      "container": "1 1 2 2",
      "visualization": "scatterplot",
      "props": {
        "data": "points"
      }
    },
    {
      "id": "chart4",
      "container": "2 2 2 2",
      "visualization": "scatterplot",
      "props": {
        "data": "points"
      }
    },
    {
      "id": "chart5",
      "container": "1 1 3 3",
      "visualization": "scatterplot",
      "props": {
        "data": "points"
      }
    },
    {
      "id": "chart6",
      "container": "2 2 3 3",
      "visualization": "graph",
      "props": {
        "data": "miserables"
      }
    }
  ],
  "coordination": [
    {
      "data": [
        { "name": "$d1", "properties": ["chart1.selection", "chart2.selection"] }
      ]
    },
    {
      "data": [
        { "name": "$d1", "properties": ["chart3.selection"] },
        { "name": "$d2", "properties": ["chart4.selection"] },
        { "name": "$d3", "properties": ["chart5.selection"] }
      ],
      "transformations": [
        {
          "name": "intersect",
          "input": ["$d1", "$d2"],
          "output": { "intersection": "$d3" }
        }
      ]
    }, 
    {
      "how": [
        "select items in chart3 and chart4",
        "intersect",
        "select in chart5 with $1"
      ]
    },
    {
      "how": [
        "select in chart1",
        "set data in others"
      ],
      "visualizations": ["chart1", "chart2", "chart3", "chart4"]
    },
    {
      "how": [
        "select in chart1",
        "navigate in chart2"
      ]
    }
  ],
  "transformations": [
    {
      "name": "kmeans",
      "url": "http://localhost:8080/comp/kmeans",
      "parameters": ["array", "k"],
      "output": ["clusters"]
    }
  ]
}
