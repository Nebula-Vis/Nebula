{
  "data": [
    {
      "name": "cars-vector",
      "path": "./data/cars-vector-array.json"
    },
    {
      "name": "empty",
      "values": []
    }
  ],
  "layout": {
    "width": "1350px",
    "height": "900px",
    "rows": [
      "1fr",
      "1fr",
      "1fr",
      "1fr",
      "1fr",
      "60px",
      "40px",
      "1fr",
      "1fr",
      "1fr",
      "1fr"
    ],
    "columns": ["220px", "1fr", "1fr", "1fr", "1fr", "1fr", "1fr", "250px"]
  },
  "visualizations": [
    {
      "id": "nav",
      "container": "1 1 1 1",
      "visualization": "dangerous-html",
      "props": {
        "html": "<b></b>"
      }
    },
    {
      "id": "select-clustering",
      "container": "6 6 2 2",
      "visualization": "select",
      "props": {
        "label": "clustering",
        "options": ["k-means", "DBSCAN", "MeanShift"],
        "selected": "k-means"
      }
    },
    {
      "id": "input",
      "container": "6 6 3 3",
      "visualization": "input",
      "props": {
        "label": "parameter"
      }
    },
    {
      "id": "button",
      "container": "6 6 4 4",
      "visualization": "button",
      "props": {
        "text": "cluster"
      }
    },
    {
      "id": "scatterplot",
      "container": "1 5 2 4",
      "visualization": "vegalite",
      "props": {
        "data": { "name": "empty" },
        "mark": "point",
        "encoding": {
          "x": { "field": "x", "type": "quantitative" },
          "y": { "field": "y", "type": "quantitative" },
          "color": { "field": "label", "type": "nominal" }
        }
      }
    }
  ],
  "coordinations": [
    "set value in select-clustering, set value in input, then when button clicked cluster with cars-vector, $1, and $2, then set data in scatterplot"
  ],
  "transformations": [
    {
      "name": "cluster",
      "url": "http://localhost:7001/api/cars/format",
      "parameters": ["vectorArray", "method", "parameter"],
      "output": ["labeled-data"]
    }
  ]
}
